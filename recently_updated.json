[
  {%- if config.extra.recently_updated_docs -%}
    {%- for mtime, rel_path, title, url in config.extra.recently_updated_docs -%}
    {
      "mtime": "{{ mtime }}",
      "rel_path": "{{ rel_path | e }}",
      "title": "{{ title | e }}",
      "url": "{{ config.site_url }}{{ url | e }}"
    }{{- "," if not loop.last -}}
    {%- endfor -%}
  {%- endif -%}
]
